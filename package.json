{
    "name": "aleph-hack-2024",
    "version": "0.0.1",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "commander": "11.1.0",
        "dotenv": "16.3.1",
        "express": "4.18.2",
        "jszip": "3.10.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "three": "0.160.0",
        "ws": "8.16.0"
    },
    "devDependencies": {
        "@parcel/packager-ts": "^2.11.0",
        "@parcel/transformer-typescript-types": "^2.11.0",
        "@types/express": "4.17.21",
        "@types/node": "20.10.6",
        "@types/react": "18.2.47",
        "@types/react-dom": "18.2.18",
        "@types/three": "0.160.0",
        "@types/ws": "8.5.10",
        "concurrently": "8.2.2",
        "esbuild": "0.19.11",
        "parcel": "2.11.0",
        "pkg": "5.8.1",
        "typescript": "5.3.3"
    },
    "scripts": {
        "build:lib": "npm run -w @alephhack/command-version build && npm run -w @alephhack/command-server-setup build && npm run -w @alephhack/command-server-start build && npm run -w @alephhack/command-server-connect build && npm run -w @alephhack/command-cdn-deploy build",
        "build:cli": "npm run -w @alephhack/cli build",
        "build": "npm run build:lib && npm run build:cli",
        "package": "npm run -w @alephhack/cli package"
    }
}
